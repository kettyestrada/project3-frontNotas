/REACT/project3-frontNotas/src/components/footer/footer.js",[],[],"/Users/virginiapena/Documents/Hackaboos_Virgi /REACT/project3-frontNotas/src/components/Footer/footer.js",[],[],"/Users/virginiapena/Documents/Hackaboos_Virgi /REACT/project3-frontNotas/src/components/Register/Register.js",["81"],[],"import { useState } from 'react';\nimport { Navigate, useNavigate } from 'react-router-dom';\nimport { useToken } from '../../TokenContext';\nimport './Register.css';\n\nconst Register = () => {\n  //   const [name, setName] = useState('');\n  const [token, setToken] = useToken();\n  const navigate = useNavigate();\n  const [email, setEmail] = useState('');\n  const [password, setPassword] = useState('');\n  const [loading, setLoading] = useState(false);\n\n  if (token) return <Navigate to=\"/\" />;\n\n  // Función que maneja el envío del formulario.\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n\n    setLoading(true);\n\n    try {\n      const res = await fetch('http://localhost:4000/user', {\n        method: 'post',\n        headers: {\n          'Content-Type': 'application/json',\n        },\n        body: JSON.stringify({\n          email,\n          password,\n        }),\n      });\n      const body = await res.json();\n\n      //si ha ido bien o ha ido mal mostramos por alert el mensaje,\n      // sea o no sea de error.\n      if (body.status === 'error') {\n        alert(body.message);\n      } else {\n        navigate('/login');\n      }\n    } catch (err) {\n      console.error(err);\n    } finally {\n      setLoading(false);\n    }\n  };\n\n  return (\n    <main className=\"Register\">\n      <h2>Registro</h2>\n      <form onSubmit={handleSubmit}>\n        <label htmlFor=\"email\">Email:</label>\n        <input\n          type=\"email\"\n          id=\"email\"\n          value={email}\n          onChange={(e) => setEmail(e.target.value)}\n          required\n          autoFocus\n        />\n        <label htmlFor=\"pass\">Contraseña:</label>\n        <input\n          type=\"password\"\n          id=\"pass\"\n          value={password}\n          onChange={(e) => setPassword(e.target.value)}\n          minLength=\"6\"\n          required\n        />\n\n        <button disabled={loading}>Registrarse</button>\n      </form>\n    </main>\n  );\n};\n\nexport default Register;\n","/Users/virginiapena/Documents/Hackaboos_Virgi /REACT/project3-frontNotas/src/components/Login/Login.js",[],[],"/Users/virginiapena/Documents/Hackaboos_Virgi /REACT/project3-frontNotas/src/TokenContext.js",[],[],"/Users/virginiapena/Documents/Hackaboos_Virgi /REACT/project3-frontNotas/src/components/main/menu.js",[],[],"/Users/virginiapena/Documents/Hackaboos_Virgi /REACT/project3-frontNotas/src/CreateNote.js",["82"],[],"/Users/virginiapena/Documents/Hackaboos_Virgi /REACT/project3-frontNotas/src/ListNotes.js",[],[],{"ruleId":"83","severity":1,"message":"84","line":11,"column":10,"nodeType":"85","messageId":"86","endLine":11,"endColumn":23},{"ruleId":"83","severity":1,"message":"87","line":8,"column":17,"nodeType":"85","messageId":"86","endLine":8,"endColumn":25},{"ruleId":"83","severity":1,"message":"88","line":5,"column":10,"nodeType":"85","messageId":"86","endLine":5,"endColumn":20},"no-unused-vars","'BrowserRouter' is defined but never used.","Identifier","unusedVar","'setToken' is assigned a value but never used.","'categories' is assigned a value but never used."]